
<div id='logged_in'>
  You're Logged In As: <%= session[:name] %>!
  <form action='/sessions/<%= session[:user] %>' method='post'>
    <input type='hidden' name='_method' value='delete'>
    <input type='submit' value='Log Out!'>
  </form>


<div id='post_peep'>
  Post a peep!
  <form action='/peep/new' method='post'>
  <textarea name='post_peep'>What are you thinking?</textarea>
  <br>
  <input type='submit' value='Peep!'>
  </form>
</div>
