<% if Peep.count > 0 %>
  <% peeps = Peep.all(order: [:id.desc]) %>
    <% peeps.each do |peep| %>
      <div class='peep'>
        <%= peep.peep %><br>
        <div class='peeped_by'> <em> Posted by User: <%= peep.user.username %> Real name: <%= peep.user.name %> </em> </div>
      </div><br>
  <% end %>
<% end %>